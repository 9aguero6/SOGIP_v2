            @Html.DisplayFor(modelItem => item.RutinaFecha)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RutinaObservaciones)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.RutinaId }) |
