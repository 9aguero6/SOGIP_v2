@model IEnumerable<SOGIP_v2.Models.Rutina>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Usuario.Cedula)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RutinaFecha)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RutinaObservaciones)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Usuario.Cedula)
        </td>
