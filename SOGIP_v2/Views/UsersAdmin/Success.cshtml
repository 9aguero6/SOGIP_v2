@{
    @Html.Partial("/Views/Home/header.cshtml")
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Success</title>
</head>
<body>
    <div class="container">
        <div class="masivo" id="registro_tabla" name="registro_tabla">
            <table cellpadding="2" cellspacing="2" border="1">
                @if (ViewBag.ListUsrs != null)
                {
                    <tr>
                        <th>Cédula</th>
                        <th>Primer Nombre</th>
                        <th>Segundo Nombre</th>
                        <th>1er Apellido</th>
                        <th>2do Apellido</th>
                        <th>Correo</th>
                        <th>Teléfono</th>
                        <th>Sexo</th>
                        <th>Fecha Nacimiento</th>
                    </tr>
                    foreach (var a in ViewBag.ListUsrs)
                    {
                        <tr>
                            <td>@a.Cedula</td>
                            <td>@a.Nombre1</td>
                            <td>@a.Nombre2</td>
                            <td>@a.Apellido1</td>
                            <td>@a.Apellido2</td>
                            <td>@a.Email</td>
                            <td>@a.PhoneNumber</td>
                            <td>@a.Sexo</td>
                            <td>@a.Fecha_Nacimiento</td>
                        </tr>
                    }
                }
            </table>
            @if (ViewBag.ListUsrs != null)
            {
                using (Html.BeginForm("Guardar", "UsersAdmin", FormMethod.Post, new { enctype = "multipart/form-data" }))
                {
                    <div>
                        <input type="submit" value="Guardar" />
                    </div>
                }
            }
        </div>
    </div>
</body>
</html>
